{
  "name": "@axelspringer/hubots",
  "version": "1.0.1",
  "description": "A TypeScript flavor version of GitHubs famous and beloved Hubot",
  "main": "dist/main.js",
  "scripts": {
    "build": "tsc -p ../../tsconfig.json --outDir ./dist",
    "build:docker": "docker build -t hubots .",
    "docs": "npm run typedoc -- --options typedoc.js --exclude '**/*.spec.ts' ./src/",
    "help": "node node_modules/npm-scripts-help",
    "typedoc": "typedoc",
    "server:dev": "./node_modules/.bin/nodemon ./dist/main.js",
    "start": "npm run build && concurrently \"npm run watch\" \"npm run server:dev\"",
    "watch": "tsc --watch -p ../../tsconfig.json --outDir ./dist"
  },
  "bin": {
    "hubot": "./bin/hubot"
  },
  "author": "Axel Springer SE",
  "contributors": [
    {
      "name": "Sebastian DÃ¶ll",
      "email": "sebastian.doell@axelspringer.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axelspringer/hubots.git"
  },
  "homepage": "https://github.com/axelspringer/hubots/tree/master/packages/hubots#README",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^4.0.5",
    "@types/koa": "^2.0.41",
    "@types/node": "^8.0.53",
    "@types/yargs": "^8.0.2",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.3",
    "nodemon": "^1.12.1",
    "npm-run-help": "^1.1.0",
    "npm-scripts-help": "^0.7.0",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typedoc": "^0.9.0",
    "typescript": "^2.6.2"
  },
  "dependencies": {
    "@koa/cors": "^2.2.1",
    "@types/chalk": "^2.2.0",
    "chalk": "^2.3.0",
    "cline": "^0.8.2",
    "coffee-script": "^1.12.7",
    "fs-extra": "^4.0.2",
    "hubot-forecastio": "^1.1.2",
    "hubot-scripts": "^2.17.2",
    "koa": "^2.4.1",
    "koa-bodyparser": "^4.2.0",
    "koa-log": "^2.1.0",
    "koa-router": "^7.3.0",
    "loglevel": "^1.6.0",
    "optparse": "^1.0.5",
    "readline": "^1.3.0",
    "scoped-http-client": "^0.11.0",
    "stream": "0.0.2",
